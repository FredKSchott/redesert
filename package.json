{
  "name": "redesert",
  "version": "0.6.3",
  "description": "Higher order functions for creating opinionated redux reducers, selectors, actions, and action creators",
  "author": "Malcolm Ahoy <malcolmahoy@gmail.com>",
  "license": "MIT",
  "distributions": {
    "src": [true],
    "plugins": [
      ["@pika/web-builder"],
      ["@pika/web-bundler"],
      ["@pika/types-builder"]
    ]
  },
  "scripts": {
    "build": "pika build",
    "clean": "rimraf lib es compiled coverage",
    "test": "jest",
    "test:ci": "jest --coverage",
    "precommit": "precise-commits",
    "publish": "pika publish"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.1",
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "@pika/pack": "^0.1.6",
    "@pika/types-builder": "^0.2.1",
    "@pika/web-builder": "^0.2.1",
    "@pika/web-bundler": "^0.2.3",
    "@types/jest": "^22.2.3",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "precise-commits": "^1.0.2",
    "prettier": "^1.12.1",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec.(ts)$",
    "testURL": "http://localhost/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/reducer-factories/test-utils/"
    ]
  },
  "peerDependencies": {
    "reselect": "^3.0.1"
  }
}
